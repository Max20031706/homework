    "def count_ways(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    elif n == 1:\n",
    "        return 1  \n",
    "    else:\n",
    "        return count_ways(n - 1) + count_ways(n - 2)\n",
    "    \n",
    "n=int(input())\n",
    "\n",
    "print(f\"Количество способов подняться на {n} ступенек равно {count_ways(n)}\")\n"
   ]
  }
